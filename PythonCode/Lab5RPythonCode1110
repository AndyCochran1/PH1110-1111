    "fig=plt.figure() #initiate the figure\n",
    "ax=fig.add_subplot(111) #So we can add multiple lines\n",
    "\n",
    "ax.plot(ball.t,ball.y, c='b',marker=\"^\",ls='--',label='First Data Set') #Data is horizontial axis, then vertical axis in blue\n",
    "ax.plot(ball.t, ball.x, c = 'r', marker = \"o\", ls = '--', label= 'Second Data Set') #plotting the linear formula in red\n",
    "\n",
    "plt.legend(loc=2) #Here you initialize your legend, which will display the label we two lines above for each data set. \n",
    "plt.draw()\n",
    "plt.title(\"First Data Set vs Second Data Set\")\n",
    "ax.set_xlabel('time (s)')\n",
    "ax.set_ylabel('position (m)')"
